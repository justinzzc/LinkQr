{  
  "name": "LinkQr",  
  "version": "1.0.0",  
  "manifest_version": 2,
  "icons": { 
    "48": "icon_48.png",
    "128":"icon_128.png"
     },
  "description": "显示当前页二维码。 Create A Qr Image for your current tab url link.",  
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "二维码－LinkQr",
    "default_popup": "popup.html"
  },
  "permissions":[
  	"tabs",
    "activeTab"
  ]
} 
